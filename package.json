{
	"name": "react-boilerplate",
	"version": "1.0.0",
	"description": "A boilerplate for future react projects.",
	"main": "index.js",
	"scripts": {
		"test": "jest --config ./jest.config.json",
		"test:all": "npm run test -- --watchAll",
		"test:watch": "npm run test -- --watch",
		"test:verbose": "npm run test -- --verbose=true --watch",
		"app": "webpack-dev-server --open",
		"lint": "prettier --write '**/*.jsx'",
		"stories": "start-storybook --port 9090",
		"start": "node server.js",
		"build": "webpack -p",
		"heroku-postbuild": "npm run build"
	},
	"author": "cl4pper",
	"license": "ISC",
	"dependencies": {
		"@svgr/webpack": "^4.3.3",
		"axios": "^0.19.0",
		"classnames": "^2.2.6",
		"file-loader": "^4.2.0",
		"moment": "^2.24.0",
		"react": "^16.11.0",
		"react-dom": "^16.11.0",
		"react-moment": "^0.9.7",
		"react-router-dom": "^5.1.2",
		"redux-thunk": "^2.3.0",
		"url-loader": "^2.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.6.4",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-transform-runtime": "^7.12.10",
		"@babel/preset-env": "^7.6.3",
		"@babel/preset-react": "^7.6.3",
		"@storybook/addon-knobs": "^5.3.9",
		"@storybook/react": "^5.3.9",
		"@testing-library/react": "^11.2.2",
		"babel-eslint": "^10.0.3",
		"babel-loader": "^8.0.5",
		"css-loader": "^2.1.1",
		"enzyme-adapter-react-16": "^1.15.5",
		"eslint": "^6.8.0",
		"eslint-plugin-react": "^7.18.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^24.9.0",
		"node-sass": "^4.13.1",
		"prop-types": "^15.7.2",
		"react-redux": "^7.1.1",
		"react-test-renderer": "^16.12.0",
		"redux": "^4.0.4",
		"sass-loader": "^7.3.1",
		"style-loader": "^0.23.1",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10",
		"webpack-dev-server": "^3.9.0"
	}
}
